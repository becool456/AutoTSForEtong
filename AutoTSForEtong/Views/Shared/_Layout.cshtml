<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 校园网络基础设施管理平台</title>
    @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
    <script src="~/Content/Chart.js"></script>
    <script src="~/Content/jquery-1.8.2.min.js"></script>
   
    <link href="~/Content/kendo/2014.1.528/kendo.common.min.css" rel="stylesheet" />
    <link href="~/Content/kendo/2014.1.528/kendo.dataviz.min.css" rel="stylesheet" />
    <link href="~/Content/kendo/2014.1.528/kendo.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/kendo/2014.1.528/kendo.dataviz.bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/kendo/2014.2.903/kendo.all.min.js"></script>
    <script src="~/Scripts/kendo/2014.2.903/kendo.aspnetmvc.min.js"></script>
    <script src="~/Scripts/kendo.modernizr.custom.js"></script>
    @*<link href="~/Content/demo.css" rel="stylesheet" />*@
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/validationEngine.jquery.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/demo.css" rel="stylesheet" />
    <link href="~/Content/flat-ui.css" rel="stylesheet" />
    <link href="~/Content/flp.css" rel="stylesheet" />
    <link href="~/Content/styles.css" rel="stylesheet" />
    <script src="~/Content/jquery.validationEngine-zh_CN.js"></script>
    <script src="~/Content/jquery.validationEngine.js"></script>
    
   
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <img src="~/Images/Clipboard.png" alt="AutoTSForETong" height="45" width="45" />
                <span style="font-family: 'Microsoft YaHei';font-size:larger;font-weight:bold;padding:20px ;color:white">自动出卷系统</span>
            </div>
            <div style="float:right">
                @Html.Action("LayoutChanger", "User")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @Html.Action("LowMenuSection", "User")
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - 二桐和他的逗比小伙伴们</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>
